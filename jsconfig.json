{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$partials": ["src/lib/partials"],
			"$partials/*": ["src/lib/partials/*"],
			"$stores": ["src/lib/stores"],
			"$stores/*": ["src/lib/stores/*"],
			"$utilities": ["src/utilities"],
			"$utilities/*": ["src/utilities/*"]
		}
	}
}
